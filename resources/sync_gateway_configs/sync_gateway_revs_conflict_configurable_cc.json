{
    "interface":":4984",
    "adminInterface": "0.0.0.0:4985",
    "maxIncomingConnections": 0,
    "maxFileDescriptors": 90000,
    "compressResponses": false,
    {{ sslcert }}
    {{ sslkey }}
    {{ logging }}
    {{ hide_product_version }}
    {{ disable_persistent_config }}
    {{ server_tls_skip_verify }}
    {{ disable_tls_server }}
    {{ disable_admin_auth }}
    "databases":{
        "db":{
            {{ autoimport }}
            {{ xattrs }}
            {{ no_conflicts }}
            {{ revs_limit }}
            {{ sg_use_views }}
            {{ num_index_replicas }}
            {{ username }}
            {{ password }}
            {{ cacertpath }}
            {{ certpath }}
            {{ keypath }}
            {{ delta_sync }}
            "rev_cache_size": 1000,
            "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}",
            "bucket":"data-bucket"
        }
    }
}
