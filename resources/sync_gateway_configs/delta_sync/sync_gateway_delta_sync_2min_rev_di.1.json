{
    "interface":":4984",
    "adminInterface": "0.0.0.0:4985",
    "maxIncomingConnections": 0,
    "maxFileDescriptors": 90000,
    "compressResponses": false,
    {{ logging }}
    "cluster_config": {
        "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}",
        "data_dir":".",
        "bucket":"travel-sample",
        "username":"travel-sample",
        "password": "password"
    },
    {{ sslcert }}
    {{ sslkey }}
    "databases":{
        "db":{
            {{ xattrs }}
            {{ no_conflicts }}
            {{ sg_use_views }}
            {{ num_index_replicas }}
            "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}",
            "bucket":"travel-sample",
            "username":"travel-sample",
            "password": "password",
            "channel_index":{
                "num_shards":16,
                "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}",
                "bucket":"index-bucket",
                "username": "index-bucket",
                "password": "password",
                "writer":{{ is_index_writer }}
            },
            "old_rev_expiry_seconds": 120,
            "delta_sync": {
                "enabled": true,
                "rev_max_age_seconds": 150
            }
        }
    }
}

