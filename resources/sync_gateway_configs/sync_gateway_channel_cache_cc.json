{
    "interface":":4984",
    "adminInterface": "0.0.0.0:4985",
    "compressResponses": false,
    {{ sslcert }}
    {{ sslkey }}
    {{ logging }}
    "databases":{
        "db": {
            {{ autoimport }}
            {{ xattrs }}
            {{ no_conflicts }}
            {{ sg_use_views }}
            {{ num_index_replicas }}
            {{ username }}
            {{ password }}
            {{ cacertpath }}
            {{ certpath }}
            {{ keypath }}
            "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}",
            "bucket":"data-bucket",
            "cache": {
                "channel_cache_max_length": 750,
                "channel_cache_min_length": 750,
                "channel_cache_expiry": 90
            }
        }
    }
}



