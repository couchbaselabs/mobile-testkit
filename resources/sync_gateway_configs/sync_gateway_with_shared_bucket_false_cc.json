{
    "interface":":4984",
    "adminInterface": "0.0.0.0:4985",
    "maxIncomingConnections": 0,
    "maxFileDescriptors": 90000,
    "compressResponses": false,
    {{ sslcert }}
    {{ sslkey }}
    {{ logging }}
    "databases":{
        "db":{
            {{ no_conflicts }}
            {{ sg_use_views }}
            {{ num_index_replicas }}
            {{ username }}
            {{ password }}
            {{ cacertpath }}
            {{ certpath }}
            {{ keypath }}
            {{ delta_sync }}
            "enable_shared_bucket_access": false,
            "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}",
            "bucket":"data-bucket",
            "cache": {
                "channel_cache":{
                "max_length": 15000,
                "min_length": 15000,
                "expiry_seconds": 90
                }
            }
        }
    }
}
