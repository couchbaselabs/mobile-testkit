{
    "interface":":4984",
    "adminInterface": "0.0.0.0:4985",
    {{ sslcert }}
    {{ sslkey }}
    {{ logging }}
    "databases":{
        "db":{
            {{ autoimport }}
            {{ xattrs }}
            {{ no_conflicts }}
            {{ sg_use_views }}
            {{ num_index_replicas }}
            "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}",
            "bucket":"data-bucket",
            "username":"data-bucket",
            "password": "password",
            "revs_limit": 100,
            "sync":
            `function(doc){
                channel(doc.channels);
            }`
        }
    }
}
