{
    "interface":":4984",
    "adminInterface": "0.0.0.0:4985",
    {{ sslcert }}
    {{ sslkey }}
    {{ logging }}
    {{ hide_product_version }}
    "databases":{
        "db":{
            {{ autoimport }}
            {{ xattrs }}
            {{ no_conflicts }}
            {{ sg_use_views }}
            {{ num_index_replicas }}
            {{ username }}
            {{ password }}
            {{ cacertpath }}
            {{ certpath }}
            {{ keypath }}
            {{ delta_sync }}
            "unsupported": {
                "remote_config_tls_skip_verify": true
            },
            "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}",
            "bucket":"data-bucket",
            "event_handlers": {
               "document_changed":[{
                    "handler": "webhook",
                    {{ webhook_filter }}
                    "url": "http://{{ webhook_ip }}:8080"
                }],
               "db_state_changed":[{
                   "handler":"webhook",
                   "url":"http://{{ webhook_ip }}:8080"
                }]
            }
        }
    }
}
