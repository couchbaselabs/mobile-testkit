{
    "bootstrap": {
        {{ server_tls_skip_verify }}
        {{ disable_tls_server }}
        {{ username }}
        {{ password }}
        {{ cacertpath }}
        {{ certpath }}
        {{ keypath }}
        {{ groupid }}
        "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}"
    },
    "logging": { "debug": {"enabled": true},
                 "audit": {"enabled": true,
                           "rotation": {"max_size": 1,
                                        "max_age": 3,
                                        "localtime": false,
                                        "rotated_logs_size_limit": 10,
                                        "enabled_events": [53260, 53270, 53271, 53272]
                                       }
                          }
                },
    "api":{
        {{ disable_admin_auth }}
        {{ tls }}
        {{ prometheus }}
        "public_interface":":4984",
        "admin_interface":"0.0.0.0:4985"
    }
}